    edge [arrowhead="none"];

    e1 -> e2 
    e1 -> p1
    e1 -> t1
    e2 -> t2
    
    t1 -> d3
    t2 -> t3
    t2 -> m1
    t2 -> d2
    t3 -> d1

    e1 [label="expr.val=19"];
    e2 [label="expr.val=15"];
    
    t1 [label="term.val=4"];
    t2 [label="term.val=15"];
    t3 [label="term.val=3"];
    
    d1 [label="DIGIT.lexval=3"];
    d2 [label="DIGIT.lexval=5"];
    d3 [label="DIGIT.lexval=4"];
    
    m1 [label="'*'"];
    p1 [label="'+'"];
}

